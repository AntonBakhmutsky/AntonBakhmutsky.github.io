header.header
  .container
    .header__logo.slide-right
      | !{require('!svg-inline-loader?removeSVGTagAttrs=false!@/assets/img/global/header-logo.svg')}
    nav.header__menu
      each el in headerMenu
        a.menu__item(href=el.url) #{el.name}
    a.header__phone.slide-up(href="tel:+79214245572")
      | !{require('!svg-inline-loader?removeSVGTagAttrs=false!@/assets/img/global/phone.svg')}
      span +7 (921) 424-55-72
