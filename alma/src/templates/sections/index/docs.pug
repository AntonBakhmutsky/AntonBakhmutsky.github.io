section.docs
  .container
    .docs-top
      h3.subtitle Лицензии
      .desktop-link
        +all-link('Все документы', 'javascript:')
    .docs-container
      each doc in docs
        .docs-card
          img(src=doc)
          .docs-card-overlay
            button.expand-button
              | !{require('!svg-inline-loader?removeSVGTagAttrs=false!@/assets/img/index/docs/expand.svg')}
    .mobile-link
      +all-link('Все документы', 'javascript:')
  .docs-modal-overlay
    .docs-modal
      button.close-button ×



